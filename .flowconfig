[ignore]
.*/packages/.*/lib
.*/packages/.*/test

[include]
packages/*/src

[libs]
.*/types/.*

[options]
suppress_comment= \\(.\\|\n\\)*\\$FlowFixMe
suppress_comment= \\(.\\|\n\\)*\\$FlowIssue
module.name_mapper='^types/\(.*\)$' -> '<PROJECT_ROOT>/types/\1.js'
module.name_mapper='\(vostok-[^/]*\)' -> '<PROJECT_ROOT>/packages/\1/src/index.js'
